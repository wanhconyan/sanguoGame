package com.core.view
{
	import com.core.util.Dispatcher;

	public class BaseModule
	{
		public function BaseModule()
		{
			
			
		}
		
		
		public  function start():void
		{
		
		}
		
		
		protected function addModuleEvent($key:String,$func:Function):void
		{
			Dispatcher.register($key,$func);
		} 
		
		protected function removeModuleEvent():void
		{
		
		
		}
	}
}
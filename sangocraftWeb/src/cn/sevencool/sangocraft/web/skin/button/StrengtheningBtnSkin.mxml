<!--- The default skin class for the Spark Button component.  

@see spark.components.Button

@langversion 3.0
@playerversion Flash 10
@playerversion AIR 1.5
@productversion Flex 4
-->
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
				   xmlns:s="library://ns.adobe.com/flex/spark"
				   xmlns:fb="http://ns.adobe.com/flashbuilder/2009" minWidth="21" minHeight="21"
				   alpha.disabled="0.5"  width="66" height="20">   
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>    
	<!-- states -->
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>		
	<s:BitmapImage smooth="true" source="@Embed('../../../../../../../assets/images/itemMenu/str_up.png')" includeIn="up,over"/>
	<s:BitmapImage smooth="true" source="@Embed('../../../../../../../assets/images/itemMenu/str_dis.png')" includeIn="disabled"/>
	<s:BitmapImage smooth="true" source="@Embed('../../../../../../../assets/images/itemMenu/str_down.png')" includeIn="down"/>
</s:SparkButtonSkin>
